<h1>Listing deploy_commands</h1>

<table>
  <thead>
    <tr>
      <th>Deploy</th>
      <th>Command</th>
      <th>Parameters</th>
      <th>Result</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @deploy_commands.each do |deploy_command| %>
      <tr>
        <td><%= deploy_command.deploy_id %></td>
        <td><%= deploy_command.command %></td>
        <td><%= deploy_command.parameters %></td>
        <td><%= deploy_command.result %></td>
        <td><%= link_to 'Show', deploy_command %></td>
        <td><%= link_to 'Edit', edit_deploy_command_path(deploy_command) %></td>
        <td><%= link_to 'Destroy', deploy_command, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Deploy command', new_deploy_command_path %>
